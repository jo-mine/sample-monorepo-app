{
  "name": "@jo-mine/excel-houganshi-to-md",
  "version": "1.0.0",
  "description": "Convert Excel houganshi (方眼紙) design documents to Markdown",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "src/index.ts",
  "scripts": {
    "dev": "bun run src/index.ts",
    "clean": "rm -rf dist",
    "build:types": "tsc --skipLibCheck --declaration --emitDeclarationOnly src/index.ts --outDir ./dist",
    "build": "bun run clean && bun run build:types && bun build src/index.ts --outdir=dist",
    "test": "bun test"
  },
  "dependencies": {
    "exceljs": "^4.4.0"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
